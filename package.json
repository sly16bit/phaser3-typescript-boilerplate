{
  "name": "phaser3-typescript-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-native": "tsc && vite build -c vite.electron.config.ts && electron-builder -c.extraMetadata.main=dist-electron/main.js",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.+(js|cjs|ts|json|md)\""
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "electron": "^30.0.2",
    "electron-builder": "^24.13.3",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "dependencies": {
    "phaser": "^3.80.1"
  }
}
